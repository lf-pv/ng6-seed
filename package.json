{
  "name": "ng6-sandbox",
  "version": "0.2.0",
  "engines": {
    "node": "~10.4.0",
    "npm": "~6.1.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "pretest": "npm run test:start-server",
    "test": "ng test --code-coverage --browsers PhantomJS --environment test --watch false --sourceMap false",
    "posttest": "npm run test:stop-server",
    "test:local": "ng test --karma-config src/karma.conf.local.js",
    "test:start-server": "pm2 start ./test/server.js",
    "test:restart-server": "pm2 restart ./test/server.js",
    "test:stop-server": "pm2 stop ./test/server.js",
    "test:delete-server": "pm2 delete ./test/server.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "npm:reinstall": "rm -rf node_modules && npm cache verify && npm i"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.4",
    "@angular/common": "6.0.4",
    "@angular/compiler": "6.0.4",
    "@angular/core": "6.0.4",
    "@angular/forms": "6.0.4",
    "@angular/http": "6.0.4",
    "@angular/platform-browser": "6.0.4",
    "@angular/platform-browser-dynamic": "6.0.4",
    "@angular/router": "6.0.4",
    "core-js": "2.5.4",
    "rxjs": "6.2.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.3",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.4",
    "@angular/language-service": "6.0.4",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "codelyzer": "4.2.1",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "json-server": "0.12.2",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "phantomjs-prebuilt": "2.1.16",
    "pm2": "2.10.4",
    "protractor": "5.3.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  }
}
